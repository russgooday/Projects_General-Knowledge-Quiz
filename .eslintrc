{
    "parserOptions": {
        "ecmaVersion": 12,
        "ecmaFeatures": {
          "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "padded-blocks": 0,
        "no-multiple-empty-lines": 0,
        "no-trailing-spaces": 0,
        "no-new": 0,
        "no-return-assign": 0
    },
    "globals": {}
}